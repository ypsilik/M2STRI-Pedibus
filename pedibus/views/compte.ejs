<%- include("_header") -%>
<%- include("_menu") -%>
<h1>Pedibus Toulouse Nord (<%= model.length %>)</h1>

<div class="table-responsive-sm">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Date</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% for (const dep of model) { %>
        <tr>
          <% if (dep.participe == 1){ %>
          <td><%= dep.date %></td>
          <td><a class="btn btn-sm btn-warning" href="/annuler/<%= dep.dep_ID %>">Annuler</a></td>
          <%}%>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>


<%- include("_footer") -%>